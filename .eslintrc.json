{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "no-unused-vars": "warn",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
